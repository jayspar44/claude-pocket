{
  "projectId": "{{FIREBASE_PROJECT_ID}}",
  "region": "{{GCP_REGION}}",
  "services": {
    "backend": {
      "dev": {
        "name": "{{BACKEND_SERVICE}}-dev",
        "hash": "{{CLOUD_RUN_HASH}}",
        "url": "https://{{BACKEND_SERVICE}}-dev-{{CLOUD_RUN_HASH}}.a.run.app"
      },
      "prod": {
        "name": "{{BACKEND_SERVICE}}-prod",
        "hash": "{{CLOUD_RUN_HASH}}",
        "url": "https://{{BACKEND_SERVICE}}-prod-{{CLOUD_RUN_HASH}}.a.run.app"
      }
    }
  },
  "firebase": {
    "hosting": {
      "dev": "{{FIREBASE_SITE_DEV}}",
      "prod": "{{FIREBASE_SITE_PROD}}"
    }
  },
  "preview": {
    "backend": {
      "urlPattern": "https://pr-{{PR_NUMBER}}---{{BACKEND_SERVICE}}-dev-{{CLOUD_RUN_HASH}}.a.run.app",
      "tagPattern": "pr-{{PR_NUMBER}}"
    },
    "frontend": {
      "urlPattern": "https://{{FIREBASE_PROJECT_ID}}--pr-{{PR_NUMBER}}-{{HASH}}.web.app",
      "channelPattern": "pr-{{PR_NUMBER}}"
    }
  }
}
