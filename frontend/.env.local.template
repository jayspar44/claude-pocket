# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Backend API URL (local development)
VITE_API_URL=http://localhost:4001/api

# Firebase client configuration (JSON)
# Get from Firebase Console > Project Settings > General > Your apps > Config
VITE_FIREBASE_CONFIG={"apiKey":"YOUR_API_KEY","authDomain":"YOUR_PROJECT_ID.firebaseapp.com","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_PROJECT_ID.appspot.com","messagingSenderId":"123456789","appId":"1:123456789:web:abc123"}
